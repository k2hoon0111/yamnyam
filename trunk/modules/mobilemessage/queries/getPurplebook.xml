<query id="getPurplebook" action="select">
	<tables>
		<table name="purplebook" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="user_id" var="user_id" notnull="notnull" pipe="and" />
		<condition operation="equal" column="node_id" var="node_id" notnull="notnull" pipe="and" />
	</conditions>
	<navigation />
</query>
