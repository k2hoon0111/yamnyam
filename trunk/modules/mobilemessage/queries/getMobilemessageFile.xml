<query id="getMobilemessageFile" action="select">
	<tables>
		<table name="mobilemessage_files" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
    <conditions>
        <condition operation="equal" column="mobilemessage_file_srl" var="mobilemessage_file_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
