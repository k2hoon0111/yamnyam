<query id="insertRecentReceiver" action="insert">
	<tables>
		<table name="mobilemessage_recent_receiver" />
	</tables>
	<columns>
		<column name="receiver_srl" var="receiver_srl" filter="number" notnull="notnull" />
		<column name="user_id" var="user_id" notnull="notnull" />
		<column name="phone_num" var="phone_num" notnull="notnull" />
		<column name="ref_name" var="ref_name" default="" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>
