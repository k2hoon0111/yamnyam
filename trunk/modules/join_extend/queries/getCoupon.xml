<query id="getCoupon" action="select">
    <tables>
        <table name="join_extend_coupon" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="code" var="coupon_code" />
        <condition operation="equal" column="coupon_srl" var="coupon_srl" pipe="and"/>
    </conditions>
</query>
