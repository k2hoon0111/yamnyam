<query id="insertInvitation" action="insert">
    <tables>
        <table name="join_extend_invitation" />
    </tables>
    <columns>
        <column name="invitation_srl" var="invitation_srl" notnull="notnull" />
        <column name="code" var="invitation_code" notnull="notnull" />
        <column name="own_member_srl" var="own_member_srl" default="0" />
        <column name="regdate" default="curdate()" />
        <column name="joindate" default="-" />
        <column name="validdate" var="validdate" />
    </columns>
</query>
