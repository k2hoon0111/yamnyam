<query id="insertCoupon" action="insert">
    <tables>
        <table name="join_extend_coupon" />
    </tables>
    <columns>
        <column name="coupon_srl" var="coupon_srl" notnull="notnull" />
        <column name="code" var="coupon_code" notnull="notnull" />
        <column name="point" var="point" notnull="notnull" />
        <column name="own_member_srl" var="own_member_srl" default="0" />
        <column name="regdate" default="curdate()" />
        <column name="joindate" default="-" />
        <column name="validdate" var="validdate" />
    </columns>
</query>
