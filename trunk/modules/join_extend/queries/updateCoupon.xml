<query id="updateCoupon" action="update">
    <tables>
        <table name="join_extend_coupon" />
    </tables>
    <columns>
        <column name="member_srl" var="member_srl" />
        <column name="joindate" var="joindate" />
    </columns>
    <conditions>
        <condition operation="equal" column="coupon_srl" var="coupon_srl"  notnull="notnull" />
    </conditions>
</query>
